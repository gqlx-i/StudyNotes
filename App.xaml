             xmlns:local="clr-namespace:StudyNotes"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/ConvertDic.xaml"/>

                <ResourceDictionary Source="/Resources/Icon.xaml"/>
                <ResourceDictionary Source="/Resources/Menu.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
